@prefix : <http://www.semanticweb.org/damians/ferrate#> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix time: <http://www.ontologydesignpatterns.org/cp/owl/timeperiod.owl#> .
@prefix vann: <http://purl.org/vocab/vann#> .
@prefix ferrate: <http://www.semanticweb.org/damians/ferrate#> .
@base <http://www.semanticweb.org/damians/ferrate> .

<http://www.semanticweb.org/damians/ferrate> rdf:type owl:Ontology ;
                                              owl:imports <http://www.ontologydesignpatterns.org/cp/owl/timeperiod.owl> ;
                                              <http://purl.org/dc/elements/1.1/creator> "Damiano Gianotti"@it ;
                                              <http://purl.org/dc/elements/1.1/date> "2020-09-05T09:00:00"^^xsd:dateTime ;
                                              <http://purl.org/dc/elements/1.1/description> """Una ontologia dal carattere relazione/guida con cui chiunque potrà acquisire maggiori conoscenze riguardanti le ferrate.
Presenza di un sistema di punteggio per la complessità, diversi gradi di difficoltà, coordinate GPS ecc..."""@it ;
                                              <http://purl.org/dc/elements/1.1/title> "Ferrata-365"@it ;
                                              <http://purl.org/vocab/vann/preferredNamespacePrefix> "ferrate:" ;
                                              <http://purl.org/vocab/vann/preferredNamespaceUri> "http://www.semanticweb.org/damians/ferrate#" ;
                                              rdfs:label "ferrate"@it .

#################################################################
#    Annotation properties
#################################################################

###  http://ontologydesignpatterns.org/opla-sd#entityPosition
<http://ontologydesignpatterns.org/opla-sd#entityPosition> rdf:type owl:AnnotationProperty .


###  http://ontologydesignpatterns.org/opla-sd#entityPositionX
<http://ontologydesignpatterns.org/opla-sd#entityPositionX> rdf:type owl:AnnotationProperty .


###  http://ontologydesignpatterns.org/opla-sd#entityPositionY
<http://ontologydesignpatterns.org/opla-sd#entityPositionY> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
<http://purl.org/dc/elements/1.1/date> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
<http://purl.org/dc/elements/1.1/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
<http://purl.org/vocab/vann/preferredNamespacePrefix> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
<http://purl.org/vocab/vann/preferredNamespaceUri> rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2003/01/geo/wgs84_pos#alt
geo:alt rdf:type owl:AnnotationProperty ;
        rdfs:comment """The WGS84 altitude of a SpatialThing (decimal meters 
above the local reference ellipsoid).""" ;
        rdfs:label "altitude" ;
        rdfs:domain geo:SpatialThing .


###  http://www.w3.org/2003/01/geo/wgs84_pos#lat
geo:lat rdf:type owl:AnnotationProperty ;
        rdfs:comment "The WGS84 latitude of a SpatialThing (decimal degrees)." ;
        rdfs:label "latitude" ;
        rdfs:domain geo:SpatialThing .


###  http://www.w3.org/2003/01/geo/wgs84_pos#location
geo:location rdf:type owl:AnnotationProperty ;
             rdfs:subPropertyOf <http://xmlns.com/foaf/0.1/based_near> ;
             rdfs:range geo:SpatialThing .


###  http://www.w3.org/2003/01/geo/wgs84_pos#long
geo:long rdf:type owl:AnnotationProperty ;
         rdfs:comment "The WGS84 longitude of a SpatialThing (decimal degrees)." ;
         rdfs:label "longitude" ;
         rdfs:domain geo:SpatialThing .


###  http://xmlns.com/foaf/0.1/based_near
<http://xmlns.com/foaf/0.1/based_near> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/timeperiod.owl#hasTimePeriodMeasurementUnit
time:hasTimePeriodMeasurementUnit rdfs:comment "Questa proprietà si riferisce ad un periodo di tempo alla sua unità di misura."@it .


###  http://www.semanticweb.org/damians/ferrate#haAvvicinamento
ferrate:haAvvicinamento rdf:type owl:ObjectProperty ;
                        rdfs:domain ferrate:Ferrata ;
                        rdfs:range ferrate:Avvicinamento ;
                        rdfs:comment "Questa proprietà mette in relazione una via ferrata con il suo avvicinamento."@it ;
                        rdfs:label "haAvvicinamento"@it .


###  http://www.semanticweb.org/damians/ferrate#haComplessità
ferrate:haComplessità rdf:type owl:ObjectProperty ;
                      owl:inverseOf ferrate:èComplessitàDi ;
                      rdfs:domain ferrate:Ferrata ;
                      rdfs:range ferrate:Complessità ;
                      rdfs:comment "Questa proprietà mette in relazione una via ferrata con la relativa complessità."@it ;
                      rdfs:label "haComplessità"@it .


###  http://www.semanticweb.org/damians/ferrate#haDifficoltà
ferrate:haDifficoltà rdf:type owl:ObjectProperty ;
                     owl:inverseOf ferrate:èDifficoltàDi ;
                     rdfs:domain ferrate:Ferrata ;
                     rdfs:range ferrate:Difficoltà ;
                     rdfs:comment "Questa proprietà mette in relazione una via ferrata con ia relativa difficoltà."@it ;
                     rdfs:label "haDifficoltà"@it .


###  http://www.semanticweb.org/damians/ferrate#haDurata
ferrate:haDurata rdf:type owl:ObjectProperty ,
                          owl:TransitiveProperty ;
                 rdfs:domain ferrate:Ferrata ;
                 rdfs:range time:TimePeriod ;
                 rdfs:comment "Questa proprietà mette in relazione una via ferrata con ia relativa durata (Time Period)."@it ;
                 rdfs:label "haDurata"@it .


###  http://www.semanticweb.org/damians/ferrate#tempo_di_percorrenza_espresso_in
ferrate:tempo_di_percorrenza_espresso_in rdf:type owl:ObjectProperty ;
                                         owl:propertyChainAxiom ( ferrate:haDurata
                                                                  time:hasTimePeriodMeasurementUnit
                                                                ) ;
                                         rdfs:comment "Questa proprietà (catena di due proprietà) mette in relazione una via ferrata con ia relativa unità di misura temporale."@it ;
                                         rdfs:label "tempo di percorrenza espresso in"@it .


###  http://www.semanticweb.org/damians/ferrate#èComplessitàDi
ferrate:èComplessitàDi rdf:type owl:ObjectProperty ;
                       rdfs:comment "Questa proprietà mette in relazione una complessità con ia relativa via ferrata."@it ;
                       rdfs:label "èComplessitàDi"@it .


###  http://www.semanticweb.org/damians/ferrate#èDifficoltàDi
ferrate:èDifficoltàDi rdf:type owl:ObjectProperty ;
                      rdfs:comment "Questa proprietà mette in relazione una difficolta con ia relativa via ferrata."@it ;
                      rdfs:label "èDifficoltàDi"@it .


###  http://www.w3.org/2003/01/geo/wgs84_pos#location
geo:location rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/timeperiod.owl#timePeriodValue
time:timePeriodValue rdfs:comment "Questa proprietà rappresenta il valore di un periodo di tempo."@it ;
                     rdfs:label "periodo di tempo"@it .


###  http://www.semanticweb.org/damians/ferrate#altitude
ferrate:altitude rdf:type owl:DatatypeProperty ;
                 rdfs:domain geo:SpatialThing ;
                 rdfs:range xsd:integer ;
                 rdfs:comment """The WGS84 altitude of a SpatialThing (decimal meters 
above the local reference ellipsoid)."""@en ;
                 rdfs:label "altitude"@en ,
                            "altitudine"@it .


###  http://www.semanticweb.org/damians/ferrate#altitudineMax
ferrate:altitudineMax rdf:type owl:DatatypeProperty ;
                      rdfs:domain ferrate:Ferrata ;
                      rdfs:range xsd:nonNegativeInteger ;
                      rdfs:comment "il punto più alto incontrato durante l'escursione."@it ;
                      rdfs:label "altitudineMax"@it .


###  http://www.semanticweb.org/damians/ferrate#durataInOre
ferrate:durataInOre rdf:type owl:DatatypeProperty ;
                    rdfs:domain time:TimePeriod ;
                    rdfs:range xsd:integer ;
                    rdfs:label "durata in ore"@it .


###  http://www.semanticweb.org/damians/ferrate#haC_Ambientali
ferrate:haC_Ambientali rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf owl:topDataProperty ;
                       rdfs:domain ferrate:Complessità ;
                       rdfs:range [ rdf:type rdfs:Datatype ;
                                    owl:onDatatype xsd:integer ;
                                    owl:withRestrictions ( [ xsd:minInclusive 1
                                                           ]
                                                           [ xsd:maxInclusive 5
                                                           ]
                                                         )
                                  ] ;
                       rdfs:comment "Le difficoltà relative all'intera escursione. La valutazione tiene conto ad esempio delle difficoltà relative all'avvicinamento, al rientro, ai tratti non protetti, all'altitudine, a possibili tratti di ghiacciai o nevai perenni, eccettera..."@it ;
                       rdfs:label "ha complessità ambientali"@it .


###  http://www.semanticweb.org/damians/ferrate#haC_Esposizione
ferrate:haC_Esposizione rdf:type owl:DatatypeProperty ;
                        rdfs:subPropertyOf owl:topDataProperty ;
                        rdfs:domain ferrate:Complessità ;
                        rdfs:range [ rdf:type rdfs:Datatype ;
                                     owl:onDatatype xsd:integer ;
                                     owl:withRestrictions ( [ xsd:minInclusive 1
                                                            ]
                                                            [ xsd:maxInclusive 5
                                                            ]
                                                          )
                                   ] ;
                        rdfs:comment "Le difficoltà relative alla sensazione di vuoto percepito durante l'ascesa."@it ;
                        rdfs:label "ha esposizione"@it .


###  http://www.semanticweb.org/damians/ferrate#haC_ImpegnoFisico
ferrate:haC_ImpegnoFisico rdf:type owl:DatatypeProperty ;
                          rdfs:subPropertyOf owl:topDataProperty ;
                          rdfs:domain ferrate:Complessità ;
                          rdfs:range [ rdf:type rdfs:Datatype ;
                                       owl:onDatatype xsd:integer ;
                                       owl:withRestrictions ( [ xsd:minInclusive 1
                                                              ]
                                                              [ xsd:maxInclusive 5
                                                              ]
                                                            )
                                     ] ;
                          rdfs:comment "Le difficoltà relative alla preparazione atletica richiesta per percorrere l'intera escursione. La valutazione tiene conto di fattori quali la lunghezza dell'itinerario, il dislivello assoluto e relativo, ecc..."@it ;
                          rdfs:label "ha impegno fisico"@it .


###  http://www.semanticweb.org/damians/ferrate#haC_Tecniche
ferrate:haC_Tecniche rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf owl:topDataProperty ;
                     rdfs:domain ferrate:Complessità ;
                     rdfs:range [ rdf:type rdfs:Datatype ;
                                  owl:onDatatype xsd:integer ;
                                  owl:withRestrictions ( [ xsd:minInclusive 1
                                                         ]
                                                         [ xsd:maxInclusive 5
                                                         ]
                                                       )
                                ] ;
                     rdfs:comment "Le difficoltà relative all'arrampicata della via attrezzata. La valutazione tiene conto di non utilizzare la fune per trazionarsi ma solo come autoassicurazione."@it ;
                     rdfs:label "ha complessità tecniche"@it .


###  http://www.semanticweb.org/damians/ferrate#latitudine
ferrate:latitudine rdf:type owl:DatatypeProperty ;
                   rdfs:domain geo:SpatialThing ;
                   rdfs:range xsd:decimal ;
                   rdfs:comment "The WGS84 latitude of a SpatialThing (decimal degrees)."@en ;
                   rdfs:label "altitude"@en ,
                              "latitudine"@it .


###  http://www.semanticweb.org/damians/ferrate#longitudine
ferrate:longitudine rdf:type owl:DatatypeProperty ;
                    rdfs:domain geo:SpatialThing ;
                    rdfs:range xsd:decimal ;
                    rdfs:comment "The WGS84 longitude of a SpatialThing (decimal degrees)."@en ;
                    rdfs:label "longitude"@en ,
                               "longitudine"@it .


###  http://www.semanticweb.org/damians/ferrate#lunghezzaItinerario
ferrate:lunghezzaItinerario rdf:type owl:DatatypeProperty ;
                            rdfs:range xsd:decimal ;
                            rdfs:comment "Lunghezza in kilometri dell'itinerario completo."@it ;
                            rdfs:label "lunghezzaItinerario"@en .


#################################################################
#    Classes
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/timeperiod.owl#TimePeriod
time:TimePeriod rdfs:comment "Un Periodo di tempo è una distanza temporale, anche approssimativa, che intercorre tra due eventi o situazioni."@it .


###  http://www.ontologydesignpatterns.org/cp/owl/timeperiod.owl#TimePeriodMeasurementUnit
time:TimePeriodMeasurementUnit rdfs:comment "L'unità di misura del periodo di tempo è l'unità di misura di un periodo di tempo (ad es. anno, mese, settimana)."@it .


###  http://www.semanticweb.org/damians/ferrate#Avvicinamento
ferrate:Avvicinamento rdf:type owl:Class ;
                      rdfs:comment "Percorso escursionistico dal punto di partenza dell'itinerario all'attacco della via attrezzata."@it ;
                      rdfs:label "Avvicinamento"@it .


###  http://www.semanticweb.org/damians/ferrate#AvvicinamentoCorto
ferrate:AvvicinamentoCorto rdf:type owl:Class ;
                           owl:equivalentClass [ owl:intersectionOf ( ferrate:Avvicinamento
                                                                      [ rdf:type owl:Restriction ;
                                                                        owl:onProperty time:hasTimePeriodMeasurementUnit ;
                                                                        owl:someValuesFrom [ rdf:type owl:Class ;
                                                                                             owl:oneOf ( ferrate:Minute
                                                                                                       )
                                                                                           ]
                                                                      ]
                                                                      [ rdf:type owl:Restriction ;
                                                                        owl:onProperty time:timePeriodValue ;
                                                                        owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                             owl:onDatatype xsd:integer ;
                                                                                             owl:withRestrictions ( [ xsd:maxExclusive 30
                                                                                                                    ]
                                                                                                                  )
                                                                                           ]
                                                                      ]
                                                                    ) ;
                                                 rdf:type owl:Class
                                               ] ;
                           rdfs:subClassOf ferrate:Avvicinamento ;
                           rdfs:comment "Sottogruppo di avvicinamenti con tempo di percorrenza uguale o inferiore a 30 minuti."@it ;
                           rdfs:label "AvvicinamentoCorto"@it .


###  http://www.semanticweb.org/damians/ferrate#Complessità
ferrate:Complessità rdf:type owl:Class ;
                    rdfs:comment "Quattro ulteriori descrizioni con valori da 1 a 5 su caratteristiche proprie della via: difficoltà tecniche, impegno fisico, esposizione e difficoltà ambientali."@it ;
                    rdfs:label "Complessità"@it .


###  http://www.semanticweb.org/damians/ferrate#ComplessitàAmbientaleElevata
ferrate:ComplessitàAmbientaleElevata rdf:type owl:Class ;
                                     rdfs:subClassOf ferrate:Complessità ;
                                     rdfs:comment "Una complessità che possiede una complessità ambientale (roccie e strapiombi) notevole (maggiore di 3)"@it ;
                                     rdfs:label "ComplessitàAmbientaleElevata"@it .


###  http://www.semanticweb.org/damians/ferrate#ComplessitàFisicaElevata
ferrate:ComplessitàFisicaElevata rdf:type owl:Class ;
                                 rdfs:subClassOf ferrate:Complessità ;
                                 rdfs:comment "Una complessità che richiede un impegno fisico importante (maggiore di 3)"@it ;
                                 rdfs:label "ComplessitàFisicaElevata"@it .


###  http://www.semanticweb.org/damians/ferrate#Difficoltà
ferrate:Difficoltà rdf:type owl:Class ;
                   owl:equivalentClass [ rdf:type owl:Class ;
                                         owl:oneOf ( ferrate:Difficile
                                                     ferrate:EstremamenteDifficile
                                                     ferrate:Facile
                                                     ferrate:ModeratamenteDifficile
                                                     ferrate:MoltoDifficile
                                                   )
                                       ] ;
                   rdfs:comment "La classificazione italiana si basa su 5 valori che tengono conto delle difficoltà complessive della via: F (facile), PD (poco/moderatamente difficile), D (difficile), TD (molto difficile) ed ED (estremamente difficile). Questa classificazione tiene in considerazione le difficoltà globali dell'escursione e non solo le difficoltà inerenti la via ferrata."@it ;
                   rdfs:label "Difficoltà"@it .


###  http://www.semanticweb.org/damians/ferrate#Ferrata
ferrate:Ferrata rdf:type owl:Class ;
                rdfs:comment "Una via ferrata è un insieme di strutture e attrezzature realizzate artificialmente su una parete rocciosa per facilitarne la salita in sicurezza in un percorso escursionistico/alpinistico; tale azione, senza la presenza e l'utilizzo delle strutture artificiali, necessiterebbe, per la progressione, della conoscenza e dell'impiego di tecniche di arrampicata in cordata con attrezzature individuali alpinistiche o corpo libero."@it ;
                rdfs:label "Ferrata"@it .


###  http://www.semanticweb.org/damians/ferrate#FerrataAltaMontagna
ferrate:FerrataAltaMontagna rdf:type owl:Class ;
                            owl:equivalentClass [ owl:intersectionOf ( ferrate:Ferrata
                                                                       [ rdf:type owl:Restriction ;
                                                                         owl:onProperty ferrate:altitudineMax ;
                                                                         owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                              owl:onDatatype xsd:nonNegativeInteger ;
                                                                                              owl:withRestrictions ( [ xsd:minExclusive "3000"^^xsd:nonNegativeInteger
                                                                                                                     ]
                                                                                                                   )
                                                                                            ]
                                                                       ]
                                                                     ) ;
                                                  rdf:type owl:Class
                                                ] ;
                            rdfs:subClassOf ferrate:Ferrata ;
                            rdfs:comment "Itinerari attrezzati che si sviluppano in ambienti di Alta Montagna, generalmente raggiungendo almeno i 3000 metri."@it ;
                            rdfs:label "FerrataAltaMontagna"@it .


###  http://www.semanticweb.org/damians/ferrate#FerrataImpegnativa
ferrate:FerrataImpegnativa rdf:type owl:Class ;
                           owl:equivalentClass [ rdf:type owl:Restriction ;
                                                 owl:onProperty ferrate:haDifficoltà ;
                                                 owl:someValuesFrom [ rdf:type owl:Class ;
                                                                      owl:oneOf ( ferrate:EstremamenteDifficile
                                                                                  ferrate:MoltoDifficile
                                                                                )
                                                                    ]
                                               ] ;
                           rdfs:subClassOf ferrate:Ferrata ;
                           rdfs:comment "Una via ferrata classificata come Difficile la cui durata è superiore a 70 minuti OR la cui difficoltà sia gradi superiori"@it ;
                           rdfs:label "FerrataImpegnativa"@it .


###  http://www.semanticweb.org/damians/ferrate#FerrataLunga
ferrate:FerrataLunga rdf:type owl:Class ;
                     owl:equivalentClass [ owl:intersectionOf ( ferrate:Ferrata
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty ferrate:lunghezzaItinerario ;
                                                                  owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                       owl:onDatatype xsd:decimal ;
                                                                                       owl:withRestrictions ( [ xsd:minExclusive 10.0
                                                                                                              ]
                                                                                                            )
                                                                                     ]
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ] ;
                     rdfs:subClassOf ferrate:Ferrata ;
                     rdfs:comment "Itinerari attrezzati di lunghezza superiore ai 10 kilometri."@it ;
                     rdfs:label "FerrataLunga"@it .


###  http://www.w3.org/2003/01/geo/wgs84_pos#Point
geo:Point rdf:type owl:Class ;
          rdfs:subClassOf geo:SpatialThing ;
          rdfs:comment "A point, typically described using a coordinate system relative to Earth, such as WGS84."@en ,
                       "Un punto, tipicamente descritto utilizzando un sistema di coordinate relative alla Terra, come WGS84."@it ,
                       """Uniquely identified by lat/long/alt. i.e.

spaciallyIntersects(P1, P2) :- lat(P1, LAT), long(P1, LONG), alt(P1, ALT),
  lat(P2, LAT), long(P2, LONG), alt(P2, ALT).

sameThing(P1, P2) :- type(P1, Point), type(P2, Point), spaciallyIntersects(P1, P2)."""@en ;
          rdfs:label "point"@en .


###  http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing
geo:SpatialThing rdf:type owl:Class ;
                 rdfs:comment """Anything with spatial extent, i.e. size, shape, or position.
 e.g. people, places, bowling balls, as well as abstract areas like cubes."""@en ,
                              """Qualsiasi cosa con estensione spaziale, cioè dimensioni, forma o posizione.
 Ad esempio persone, luoghi, palle da bowling, così come aree astratte come i cubi."""@it ;
                 rdfs:label "SpatialThing"@en .


#################################################################
#    Individuals
#################################################################

###  http://www.semanticweb.org/damians/ferrate#Alpini_Oranaye
ferrate:Alpini_Oranaye rdf:type owl:NamedIndividual ,
                                ferrate:Ferrata ;
                       ferrate:haAvvicinamento ferrate:Alpini_OranayeAvvicinamento ;
                       ferrate:haComplessità ferrate:Alpini_OranayeComplessità ;
                       ferrate:haDifficoltà ferrate:Difficile ;
                       ferrate:haDurata ferrate:Alpini_OranayeDurataFerrata ;
                       ferrate:altitude 1720 ;
                       ferrate:altitudineMax 3050 ;
                       ferrate:latitudine 44.43882 ;
                       ferrate:longitudine 6.98932 ;
                       ferrate:lunghezzaItinerario 12.6 ;
                       rdfs:comment """La stupenda via Ferrata degli Alpini all'Oronaye si sviluppa su una strada fatta dagli alpini per scopi militari al confine con la Francia.
L'itinerario è lungo, di alta montagna."""@it ;
                       rdfs:label "Alpini_Oranaye"@it ;
                       geo:alt 1720 ;
                       geo:lat 44.43882 ;
                       geo:long 6.98932 .


###  http://www.semanticweb.org/damians/ferrate#Alpini_OranayeAvvicinamento
ferrate:Alpini_OranayeAvvicinamento rdf:type owl:NamedIndividual ;
                                    time:hasTimePeriodMeasurementUnit ferrate:Minute ;
                                    time:timePeriodValue 180 ;
                                    rdfs:comment """Proseguendo sulla strada in cui abbiamo parcheggiato giungiamo a un cartello che indica il sentiero S9 che ci porterà nel vallone di Enchiausa.

Si prosegue camminando lungo la valle in direzione dell'evidente cima del Monte Oronaye in fondo al vallone. Si prosegue per sentiero via via più ripido fino a un tratto pianeggiante in cui prenderemo il sentiero S30 verso sinistra in prossimità di un bivio.

Si pende verso sinistra in direzione dell'Oronaye giungendo quindi al Bivacco Enrico Mario (2650 m). Nei pressi del Bivacco c'è un cartello con descrizione della via ferrata. 

Si indossa l'imbraco e si parte in direzione si un primo salto roccioso senza protezioni, ma non difficile. Si costeggia la montagna verso destra lungo ghiaioni non propriamente stabili giungendo quindi all'attacco della ferrata seguendo le frecce gialle lungo il percorso."""@it ;
                                    rdfs:label "Alpini_OranayeAvvicinamento"@it .


###  http://www.semanticweb.org/damians/ferrate#Alpini_OranayeComplessità
ferrate:Alpini_OranayeComplessità rdf:type owl:NamedIndividual ;
                                  ferrate:haC_Ambientali 4 ;
                                  ferrate:haC_Esposizione 3 ;
                                  ferrate:haC_ImpegnoFisico 4 ;
                                  ferrate:haC_Tecniche 2 ;
                                  rdfs:comment "L'itinerario è con pochi punti di appoggio quindi da percorrere in condizioni meteo stabili e buon allenamento."@it ;
