    <!-- http://www.ontologydesignpatterns.org/cp/owl/timeperiod.owl#TimePeriod -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/cp/owl/timeperiod.owl#TimePeriod">
        <rdfs:comment xml:lang="en">A Time Period is a temporal distance, even approximated, that elapses between two events or situations.</rdfs:comment>
        <rdfs:comment xml:lang="it">Un Periodo di tempo è una distanza temporale, anche approssimativa, che intercorre tra due eventi o situazioni.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/timeperiod.owl"/>
        <rdfs:label xml:lang="en">Time Period</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/timeperiod.owl#TimePeriodMeasurementUnit -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/cp/owl/timeperiod.owl#TimePeriodMeasurementUnit">
        <rdfs:comment xml:lang="it">L&apos;unità di misura del periodo di tempo è l&apos;unità di misura di un periodo di tempo (ad es. anno, mese, settimana).</rdfs:comment>
        <rdfs:comment xml:lang="en">The Time Period Measurement Unit is the unit for measuring a Time Period (e.g. year, month, week).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/timeperiod.owl"/>
        <rdfs:label xml:lang="en">Time Period Measurement Unit</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/damians/ferrate#Avvicinamento -->

    <owl:Class rdf:about="http://www.semanticweb.org/damians/ferrate#Avvicinamento">
        <rdfs:comment xml:lang="it">Percorso escursionistico dal punto di partenza dell&apos;itinerario all&apos;attacco della via attrezzata.</rdfs:comment>
        <rdfs:label xml:lang="it">Avvicinamento</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/damians/ferrate#AvvicinamentoCorto -->

    <owl:Class rdf:about="http://www.semanticweb.org/damians/ferrate#AvvicinamentoCorto">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/timeperiod.owl#TimePeriod"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/damians/ferrate#Avvicinamento"/>
        <rdfs:comment xml:lang="it">Sottogruppo di avvicinamenti con tempo di percorrenza uguale o inferiore a 30 minuti.</rdfs:comment>
        <rdfs:label xml:lang="it">AvvicinamentoCorto</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/damians/ferrate#Complessità -->

    <owl:Class rdf:about="http://www.semanticweb.org/damians/ferrate#Complessità">
        <rdfs:comment xml:lang="it">Quattro ulteriori descrizioni con valori da 1 a 5 su caratteristiche proprie della via: difficoltà tecniche, impegno fisico, esposizione e difficoltà ambientali.</rdfs:comment>
        <rdfs:label xml:lang="it">Complessità</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/damians/ferrate#ComplessitàAmbientaleElevata -->

    <owl:Class rdf:about="http://www.semanticweb.org/damians/ferrate#ComplessitàAmbientaleElevata">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/damians/ferrate#Complessità"/>
        <rdfs:comment xml:lang="it">Una complessità che possiede una complessità ambientale (roccie e strapiombi) notevole (maggiore di 3)</rdfs:comment>
        <rdfs:label xml:lang="it">ComplessitàAmbientaleElevata</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/damians/ferrate#ComplessitàFisicaElevata -->

    <owl:Class rdf:about="http://www.semanticweb.org/damians/ferrate#ComplessitàFisicaElevata">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/damians/ferrate#Complessità"/>
        <rdfs:comment xml:lang="it">Una complessità che richiede un impegno fisico importante (maggiore di 3)</rdfs:comment>
        <rdfs:label xml:lang="it">ComplessitàFisicaElevata</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/damians/ferrate#Difficoltà -->

    <owl:Class rdf:about="http://www.semanticweb.org/damians/ferrate#Difficoltà">
        <rdfs:comment xml:lang="it">La classificazione italiana si basa su 5 valori che tengono conto delle difficoltà complessive della via: F (facile), PD (poco/moderatamente difficile), D (difficile), TD (molto difficile) ed ED (estremamente difficile). Questa classificazione tiene in considerazione le difficoltà globali dell&apos;escursione e non solo le difficoltà inerenti la via ferrata.</rdfs:comment>
        <rdfs:label xml:lang="it">Difficoltà</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/damians/ferrate#Ferrata -->

    <owl:Class rdf:about="http://www.semanticweb.org/damians/ferrate#Ferrata">
        <rdfs:comment xml:lang="it">Una via ferrata è un insieme di strutture e attrezzature realizzate artificialmente su una parete rocciosa per facilitarne la salita in sicurezza in un percorso escursionistico/alpinistico; tale azione, senza la presenza e l&apos;utilizzo delle strutture artificiali, necessiterebbe, per la progressione, della conoscenza e dell&apos;impiego di tecniche di arrampicata in cordata con attrezzature individuali alpinistiche o corpo libero.</rdfs:comment>
        <rdfs:label xml:lang="it">Ferrata</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/damians/ferrate#FerrataAltaMontagna -->

    <owl:Class rdf:about="http://www.semanticweb.org/damians/ferrate#FerrataAltaMontagna">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/damians/ferrate#Ferrata"/>
        <rdfs:comment xml:lang="it">Itinerari attrezzati che si sviluppano in ambienti di Alta Montagna, generalmente raggiungendo almeno i 3000 metri.</rdfs:comment>
        <rdfs:label xml:lang="it">FerrataAltaMontagna</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/damians/ferrate#FerrataImpegnativa -->

    <owl:Class rdf:about="http://www.semanticweb.org/damians/ferrate#FerrataImpegnativa">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/damians/ferrate#Ferrata"/>
        <rdfs:comment xml:lang="it">Una via ferrata classificata come Difficile la cui durata è superiore a 70 minuti OR la cui difficoltà sia gradi superiori</rdfs:comment>
        <rdfs:label xml:lang="it">FerrataImpegnativa</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/damians/ferrate#FerrataLunga -->

    <owl:Class rdf:about="http://www.semanticweb.org/damians/ferrate#FerrataLunga">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/damians/ferrate#Ferrata"/>
        <rdfs:comment xml:lang="it">Itinerari attrezzati di lunghezza superiore ai 10 kilometri.</rdfs:comment>
        <rdfs:label xml:lang="it">FerrataLunga</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/2003/01/geo/wgs84_pos#Point -->

    <owl:Class rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#Point">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing"/>
        <rdfs:comment xml:lang="en">A point, typically described using a coordinate system relative to Earth, such as WGS84.</rdfs:comment>
        <rdfs:comment xml:lang="it">Un punto, tipicamente descritto utilizzando un sistema di coordinate relative alla Terra, come WGS84.</rdfs:comment>
        <rdfs:comment xml:lang="en">Uniquely identified by lat/long/alt. i.e.

spaciallyIntersects(P1, P2) :- lat(P1, LAT), long(P1, LONG), alt(P1, ALT),
  lat(P2, LAT), long(P2, LONG), alt(P2, ALT).

sameThing(P1, P2) :- type(P1, Point), type(P2, Point), spaciallyIntersects(P1, P2).</rdfs:comment>
        <rdfs:label xml:lang="en">point</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing -->

    <owl:Class rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing">
        <rdfs:comment xml:lang="en">Anything with spatial extent, i.e. size, shape, or position.
 e.g. people, places, bowling balls, as well as abstract areas like cubes.</rdfs:comment>
        <rdfs:comment xml:lang="it">Qualsiasi cosa con estensione spaziale, cioè dimensioni, forma o posizione.
 Ad esempio persone, luoghi, palle da bowling, così come aree astratte come i cubi.</rdfs:comment>
        <rdfs:label xml:lang="en">SpatialThing</rdfs:label>
    </owl:Class>
    


